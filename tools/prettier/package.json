{
  "name": "@nizhdanov/prettier",
  "type": "module",
  "version": "0.0.2",
  "description": "prettier config",
  "author": {
    "name": "nizhdanov",
    "url": "https://github.com/nizhdanov"
  },
  "license": "MIT",
  "homepage": "https://github.com/nizhdanov/core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nizhdanov/core.git",
    "directory": "tools/prettier"
  },
  "bugs": {
    "url": "https://github.com/nizhdanov/core/issues"
  },
  "keywords": [
    "prettier",
    "nizhdanov",
    "config"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "build": "tsup src/index.ts --format esm --dts --minify",
    "format": "prettier . --write",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@nizhdanov/typescript": "workspace:*",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "prettier": ">=3.0.0 <4",
    "typescript": ">=5.0.0 <6"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  }
}
